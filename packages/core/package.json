{
  "name": "@pwops/core",
  "version": "0.1.4",
  "description": "Core package for pwops, agnostic CSS-in-JS facilitator",
  "keywords": [
    "css-in-js",
    "styled-components",
    "emotion",
    "glamor",
    "inline-styles",
    "react"
  ],
  "author": {
    "name": "leny",
    "email": "info@flatland.be",
    "url": "https://leny.me"
  },
  "homepage": "https://github.com/leny/pwops",
  "repository": "https://github.com/leny/pwops/tree/master/packages/core",
  "files": [
    "lib"
  ],
  "bugs": {
    "url": "https://github.com/leny/pwops/issues"
  },
  "license": "MIT",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "scripts": {
    "prepare": "node scripts/extract-mdn-data.js",
    "build:cjs": "npx babel src --env-name cjs --out-dir lib/cjs",
    "build:esm": "npx babel src --env-name esm --out-dir lib/esm",
    "build": "npm run build:cjs && npm run build:esm",
    "pretest": "npm run prepare && npm run build",
    "test": "npx mocha --require @babel/register",
    "lint": "npx eslint --fix --cache src"
  },
  "lint-staged": {
    "*.js": [
      "npx eslint --fix --cache",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "assert": "^1.4.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.4",
    "mdn-data": "^2.0.2",
    "mocha": "^6.0.2",
    "prettier": "^1.10.2"
  },
  "dependencies": {
    "camelcase": "^5.3.1",
    "lodash.frompairs": "^4.0.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "f31f343b9740e15df457904b454dd29b97d52e7a"
}
