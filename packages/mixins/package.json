{
  "name": "@pwops/mixins",
  "version": "0.0.3",
  "description": "Mixins for pwops, agnostic CSS-in-JS facilitator",
  "keywords": [
    "css-in-js",
    "styled-components",
    "emotion",
    "glamor",
    "inline-styles",
    "react",
    "mixins"
  ],
  "author": {
    "name": "leny",
    "email": "info@flatland.be",
    "url": "https://leny.me"
  },
  "homepage": "https://github.com/leny/pwops",
  "repository": "https://github.com/leny/pwops/tree/master/packages/mixins",
  "files": [
    "lib"
  ],
  "bugs": {
    "url": "https://github.com/leny/pwops/issues"
  },
  "license": "MIT",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "scripts": {
    "build:cjs": "npx babel src --env-name cjs --out-dir lib/cjs",
    "build:esm": "npx babel src --env-name esm --out-dir lib/esm",
    "build": "npm run build:cjs && npm run build:esm",
    "pretest": "npm run build",
    "test": "npx mocha --require @babel/register",
    "lint": "npx eslint --fix --cache src"
  },
  "lint-staged": {
    "*.js": [
      "npx eslint --fix --cache",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "@pwops/core": "^0.0.3",
    "assert": "^1.4.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.15.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.4",
    "mocha": "^6.0.2",
    "prettier": "^1.10.2"
  },
  "dependencies": {
    "lodash.frompairs": "^4.0.1"
  },
  "peerDependencies": {
    "@pwops/core": "^0.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
